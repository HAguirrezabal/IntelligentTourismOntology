        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/Coordinates"/>
        <rdfs:comment xml:lang="en">Relates a city to its geographical coordinates.</rdfs:comment>
        <rdfs:label xml:lang="en">has coordinates</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#hasCuisineStyle -->

