        <rdfs:isDefinedBy xml:lang="en">http://xmlns.com/foaf/0.1/name</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">foaf:name</rdfs:label>
    </owl:DatatypeProperty>
    


