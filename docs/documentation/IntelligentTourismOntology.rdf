        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">In this case use as a Datatype double instead of literal despite of the definition of the ontology reused.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property represents the x coordinate, that is the longitude.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://w3id.org/arco/ontology/location/long</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">longitude</rdfs:label>
    </owl:DatatypeProperty>
    


