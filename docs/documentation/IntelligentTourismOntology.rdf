        <rdfs:range rdf:resource="http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#CuisineStyle"/>
        <rdfs:comment xml:lang="en">Indicates the cuisine style offered by a restaurant</rdfs:comment>
        <rdfs:label xml:lang="en">has cuisine style</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#hasHotelRating -->

