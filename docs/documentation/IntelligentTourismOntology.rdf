    <!-- http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#areCoordinatesOf -->

    <owl:ObjectProperty rdf:about="http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#areCoordinatesOf">
        <owl:inverseOf rdf:resource="http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#hasCoordinates"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/Coordinates"/>
        <rdfs:range rdf:resource="https://dbpedia.org/ontology/City#City"/>
        <rdfs:comment xml:lang="en">Relates a set of coordinates with a city.</rdfs:comment>
        <rdfs:label xml:lang="en">are coordinates of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#cityOf -->

    <owl:ObjectProperty rdf:about="http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#cityOf">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#Event"/>
                    <rdf:Description rdf:about="http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#TouristicAttraction"/>
                    <rdf:Description rdf:about="https://dbpedia.org/ontology/Building"/>
                    <rdf:Description rdf:about="https://dbpedia.org/ontology/MeanOfTransportation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://dbpedia.org/ontology/City#City"/>
    </owl:ObjectProperty>
    


    <!-- http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#hasAttractionRating -->

    <owl:ObjectProperty rdf:about="http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#hasAttractionRating">
        <rdfs:domain rdf:resource="http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#TouristicAttraction"/>
        <rdfs:range rdf:resource="http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#Rating"/>
        <rdfs:comment xml:lang="en">Relates an attraction to its associated rating value.</rdfs:comment>
        <rdfs:label xml:lang="en">has attraction rating</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#hasCoordinates -->

    <owl:ObjectProperty rdf:about="http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#hasCoordinates">
