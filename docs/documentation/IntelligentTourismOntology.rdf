    <!-- http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#Latitude -->

    <owl:DatatypeProperty rdf:about="http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#Latitude">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/Coordinates"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">In this case use as a Datatype double instead of literal despite of the definition of the ontology reused.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property represents the y coordinate, that is the latitude.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://w3id.org/arco/ontology/location/lat</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">latitude</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#Longitude -->

    <owl:DatatypeProperty rdf:about="http://uoc.edu/knowledgeManagement/IntelligentTourismOntology#Longitude">
