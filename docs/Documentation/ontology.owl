    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#areCoordinatesOf -->


    <owl:ObjectProperty rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#areCoordinatesOf">
        <owl:inverseOf rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#hasCoordinates"/>
        <rdfs:domain rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Coordinates"/>
        <rdfs:range rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#City"/>
        <rdfs:comment xml:lang="en">Relates a set of coordinates with a city.</rdfs:comment>
        <rdfs:label xml:lang="en">are coordinates of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#cityOf -->


    <owl:ObjectProperty rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#cityOf">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Building"/>
                    <rdf:Description rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Event"/>
                    <rdf:Description rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#MeanOfTransportation"/>
                    <rdf:Description rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#TouristicAttraction"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#City"/>
        <rdfs:comment xml:lang="en">Relates the city where a resource is locate.</rdfs:comment>
        <rdfs:label xml:lang="en">city of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#hasAttractionRating -->


    <owl:ObjectProperty rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#hasAttractionRating">
