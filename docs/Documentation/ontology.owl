        <rdfs:label xml:lang="en">rating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#starRating -->


    <owl:DatatypeProperty rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#starRating">
