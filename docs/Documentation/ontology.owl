        <rdfs:comment xml:lang="en">Indicates the average price of a transit by a mean of transportation.</rdfs:comment>
        <rdfs:label xml:lang="en">average price per transit</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#name -->


    <owl:DatatypeProperty rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#name">
