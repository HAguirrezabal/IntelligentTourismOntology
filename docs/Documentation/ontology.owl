    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Andalusian -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Andalusian">
        <rdfs:subClassOf rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#CuisineStyle"/>
        <rdfs:comment xml:lang="en">Andalusian cuisine is the style of cuisine which have its origin in the gastronomy tradition of the Spanish region of Andalucía.</rdfs:comment>
        <rdfs:label xml:lang="en">Andalusian Cuisine</rdfs:label>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Beach -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Beach">
        <rdfs:subClassOf rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#TouristicAttractionType"/>
        <rdfs:comment xml:lang="en">A beach is a natural landform along the shoreline of an ocean, sea, lake, or river, often visited for recreation and relaxation.</rdfs:comment>
        <rdfs:label xml:lang="en">Beach</rdfs:label>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#BeachHotel -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#BeachHotel">
        <rdfs:subClassOf rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#HotelType"/>
        <rdfs:comment xml:lang="en">Beach hotel is the type of hotel which has its origin in tourism facilities located near or on the coast, designed for beach-related activities.</rdfs:comment>
        <rdfs:label xml:lang="en">Beach Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Bicycle -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Bicycle">
        <rdfs:subClassOf rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#MeanOfTransportationType"/>
        <rdfs:comment xml:lang="en">A bicycle is a human-powered two-wheeled vehicle used for personal transportation, especially in urban and recreational contexts.</rdfs:comment>
        <rdfs:label xml:lang="en">Bicycle</rdfs:label>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Building -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Building">
        <rdfs:comment xml:lang="en">Building is defined as a Civil Engineering structure such as a house, worship center, factory etc. that has a foundation, wall, roof etc. that protect human being and their properties from direct harsh effect of weather like rain, wind, sun etc. (http://en.wikipedia.org/wiki/Building).</rdfs:comment>
        <rdfs:label xml:lang="en">Building</rdfs:label>
        <rdfs:sameAs xml:lang="en">https://dbpedia.org/ontology/Building</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Bus -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Bus">
        <rdfs:subClassOf rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#MeanOfTransportationType"/>
        <rdfs:comment xml:lang="en">A bus is a large motor vehicle designed to carry many passengers, usually following fixed routes and schedules.</rdfs:comment>
        <rdfs:label xml:lang="en">Bus</rdfs:label>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#BusinessHotel -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#BusinessHotel">
        <rdfs:subClassOf rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#HotelType"/>
        <rdfs:comment xml:lang="en">Business hotel is the type of hotel which has its origin in the hospitality services focused on corporate or professional travelers.</rdfs:comment>
        <rdfs:label xml:lang="en">Business Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#CheapRestaurant -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#CheapRestaurant">
        <rdfs:subClassOf rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Restaurant"/>
        <rdfs:comment xml:lang="en">The price of the average meals is equal or inferior to 15.</rdfs:comment>
        <rdfs:label xml:lang="en">Cheap Restaurant</rdfs:label>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#City -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#City">
        <rdfs:comment xml:lang="en">A city is a large human settlement.</rdfs:comment>
        <rdfs:label xml:lang="en">City</rdfs:label>
        <rdfs:sameAs xml:lang="en">https://dbpedia.org/ontology/city</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Coordinates -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Coordinates">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Latitude"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Longitude"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Rating"/>
        <rdfs:comment xml:lang="en">This class represents the set of coordinates, geographic or projected, which allows the georeferencing of an object. For the punctual georeferencing there is only a pair of coordinates. For the linear and areal georeferencing there are more pairs of coordinates identifying those points of the line or area representing the object.</rdfs:comment>
        <rdfs:label xml:lang="en">a-loc:Coordinates</rdfs:label>
        <rdfs:sameAs xml:lang="en">https://w3id.org/arco/ontology/location/Coordinates</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#CuisineStyle -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#CuisineStyle">
        <rdfs:comment xml:lang="en">Cuisine Style is a national cuisine of a food or restaurant.</rdfs:comment>
        <rdfs:label xml:lang="en">CuisineStyle</rdfs:label>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#CulturalAttraction -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#CulturalAttraction">
        <rdfs:subClassOf rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#TouristicAttractionType"/>
        <rdfs:comment xml:lang="en">Cultural Atraction is a place or event that showcases the heritage, traditions, art, or history of a society or region.</rdfs:comment>
        <rdfs:label xml:lang="en">Cultural Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#CulturalEvent -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#CulturalEvent">
        <rdfs:subClassOf rdf:resource="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#EventType"/>
        <rdfs:comment xml:lang="en">A cultural event is an occasion that promotes or celebrates the arts, heritage, or traditions of a community or region.</rdfs:comment>
        <rdfs:label xml:lang="en">CulturalEvent</rdfs:label>
    </owl:Class>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Event -->


    <owl:Class rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#Event">
        <rdfs:comment xml:lang="en">An event is a temporary and scheduled happening, like a conference, festival, competition or similar</rdfs:comment>
        <rdfs:label xml:lang="en">Event</rdfs:label>
