        <terms:description xml:lang="en">Ontology that models key entities involved in intelligent tourism such as hotels, restaurants, events, and transportation.</terms:description>
        <terms:license xml:lang="en">4.0</terms:license>
        <terms:title xml:lang="en">Intelligent Tourism Ontology</terms:title>
        <vann:preferredNamespacePrefix xml:lang="en">ito</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri xml:lang="en">https://github.com/HAguirrezabal/IntelligentTourismOntology/blob/main/docs/Documentation/ontology.owl</vann:preferredNamespaceUri>
