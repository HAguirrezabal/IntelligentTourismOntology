        <rdfs:isDefinedBy xml:lang="en">http://dbpedia.org/ontology/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">star rating</rdfs:label>
        <rdfs:sameAs xml:lang="en">https://dbpedia.org/ontology/starRating</rdfs:sameAs>
    </owl:DatatypeProperty>
    


