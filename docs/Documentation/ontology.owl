        <rdfs:label xml:lang="en">Intelligent Tourism Ontology</rdfs:label>
        <owl:versionIRI xml:lang="en">http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology/1.0.0</owl:versionIRI>
