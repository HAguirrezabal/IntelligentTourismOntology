        <rdfs:label xml:lang="en">name</rdfs:label>
        <rdfs:sameAs xml:lang="en">http://xmlns.com/foaf/spec/#term_name</rdfs:sameAs>
    </owl:DatatypeProperty>
    


    <!-- http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#rating -->


    <owl:DatatypeProperty rdf:about="http://uoc.edu/knowledgeRepresentation/IntelligentTourismOntology#rating">
